  "author": "Simon Y. Blackwell <syblackwell@anywhichway.com> (http://www.github.com/anywhichway)",
  "bugs": {
    "url": "https://github.com/anywhichway/dexterous/issues"
  },
  "homepage": "https://github.com/anywhichway/dexterous#readme",
  "devDependencies": {
    "curl": "^0.1.4",
    "webpack": "^4.10.2"
  },
  "dependencies": {
    "path-to-regexp": "^2.2.1",
    "wsfetch": "0.0.2"
  }
}
